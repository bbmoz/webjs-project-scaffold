{
  "name": "example-project",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "pretest": "eslint *.js src/ test/",
    "test": "ava",
    "test:fn:setup": "./misc/ngrok/ngrok.sh && hotel add 'npm start' -n app -e NODE_ENV; hotel add -n selenium-hub-logs 'docker logs selenium-hub -f'; hotel add -n selenium-node-chrome-logs 'docker logs seleniumgridcc_selenium-chrome_1 -f'; hotel add -n selenium-node-firefox-logs 'docker logs seleniumgridcc_selenium-firefox_1 -f'",
    "test:fn:tunnel": "ngrok start app",
    "test:fn": "codeceptjs run --steps",
    "start": "nodemon --watch webpack.config.js --exec 'webpack-dev-server --content-base dist/ --hot --inline'",
    "build": "rm -rf dist/ && webpack -p --bail --progress"
  },
  "author": "",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-latest": "^6.24.0",
    "codeceptjs": "^0.5.1",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "hotel": "^0.7.1",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "selenium-webdriver": "^3.3.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failWithoutAssertions": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
